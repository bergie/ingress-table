{
  "name": "ingress-table",
  "version": "2.0.1",
  "description": "Ingress Table with NoFlo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/c-base/ingress-table.git"
  },
  "dependencies": {
    "google-auth-library": "~0.10.0",
    "microflo": "~0.3.36",
    "noflo": "~0.8.0",
    "noflo-core": "~0.4.0",
    "noflo-filesystem": "~1.1.1",
    "noflo-mqtt": "~0.3.0",
    "noflo-objects": "~0.3.0",
    "noflo-packets": "~0.1.7",
    "noflo-runtime": "~0.4.0",
    "noflo-strings": "~0.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "fbp-spec": "^0.2.0",
    "grunt": "^1.0.1",
    "grunt-coffeelint": "^0.0.16",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^2.0.0",
    "grunt-mocha-test": "^0.13.2",
    "grunt-noflo-lint": "^0.3.0",
    "microflo-emscripten": "^0.3.28",
    "mocha": "^3.1.2",
    "noflo-nodejs": "^0.8.1"
  },
  "scripts": {
    "test": "grunt test",
    "postinstall": "./node_modules/.bin/noflo-cache-preheat"
  },
  "noflo": {
    "runtimes": [
      {
        "id": "PortalLights",
        "label": "Portal Lights",
        "description": "Handles portal LED strip (Arduino)",
        "icon": "lightbulb-o",
        "type": "microflo",
        "protocol": "microflo",
        "graph": "./graphs/PortalLights.fbp",
        "address": "simulator://"
      },
      {
        "id": "StreetLights",
        "label": "Street Lights",
        "icon": "lightbulb-o",
        "description": "Handles street and ambient LED lights (Tiva)",
        "type": "microflo",
        "protocol": "microflo",
        "graph": "./graphs/TableLights.fbp",
        "address": "simulator://"
      }
    ]
  },
  "microflo": {
    "graphs": {
      "TableLights": "graphs/TableLights.fbp",
      "PortalLights": "graphs/PortalLights.fbp"
    }
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "noflo"
  ]
}
