{
  "name": "ingress-table",
  "version": "0.0.0",
  "description": "Ingress Table with NoFlo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/c-base/ingress-table.git"
  },
  "dependencies": {
    "noflo": "~0.5.0",
    "noflo-core": "~0.1.5",
    "noflo-filesystem": "~1.0.5",
    "noflo-webserver": "~0.0.4",
    "noflo-websocket": "~0.1.3",
    "noflo-strings": "^0.1.6",
    "noflo-objects": "^0.1.9",
    "microflo": "^0.3.14",
    "noflo-packets": "^0.1.3",
    "noflo-mqtt": "~0.1.0",
    "noflo-runtime": "~0.1.2"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "coffee-script": "~1.8.0",
    "grunt": "^0.4.4",
    "grunt-cafe-mocha": "^0.1.12",
    "grunt-coffeelint": "0.0.13",
    "grunt-contrib-coffee": "^0.10.1",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-exec": "^0.4.6",
    "grunt-noflo-browser": "^0.1.9",
    "grunt-noflo-manifest": "^0.1.2",
    "mocha": "^2.0.1",
    "noflo-nodejs": "~0.0.6"
  },
  "scripts": {
    "test": "grunt test"
  },
  "noflo": {
    "icon": "beer",
    "components": {
      "CalculateState": "components/CalculateState.coffee",
      "ConvertStreetLight": "components/ConvertStreetLight.coffee",
      "DetectAttack": "components/DetectAttack.coffee",
      "DetermineWinner": "components/DetermineWinner.coffee",
      "GetPortalStates": "components/GetPortalStates.coffee",
      "PortalToMessage": "components/PortalToMessage.coffee",
      "Pulsate": "components/Pulsate.coffee",
      "StabilizePortal": "components/StabilizePortal.coffee",
      "StateToColor": "components/StateToColor.coffee",
      "StateToTransmission": "components/StateToTransmission.coffee"
    },
    "graphs": {
      "ConfigPaths": "graphs/ConfigPaths.json",
      "FetchDat": "graphs/FetchDat.json",
      "PrepareMessage": "graphs/PrepareMessage.json",
      "bgt9b": "graphs/bgt9b.json"
    },
    "runtimes": [
      {
        "id": "PortalLights",
        "label": "Portal Lights",
        "description": "Handles portal LED strip",
        "type": "microflo",
        "protocol": "microflo",
        "address": "serial:///dev/ttyACM1"
      },
      {
        "id": "StreetLights",
        "label": "Street Lights",
        "description": "Handles street and ambient LED lights",
        "type": "microflo",
        "protocol": "microflo",
        "address": "serial:///dev/ttyACM0"
      }
    ]
  },
  "microflo": {
    "graphs": {
      "TableLights": "graphs/TableLights.fbp",
      "PortalLights": "graphs/PortalLights.fbp"
    }
  },
  "author": "",
  "license": "MIT"
}
